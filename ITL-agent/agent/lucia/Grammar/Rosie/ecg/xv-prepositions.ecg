//  Prepositions
construction BELOW
   subcase of Preposition
   form
      constraints
         self.f.orth <-- "below"
   meaning
      constraints
         self.m.name <-- "below1"

construction FOR
   subcase of AdverbialPreposition
   form
      constraints
         self.f.orth <-- "for"
   meaning
      constraints
         self.m.name <-- "for"

construction HOLDING
   subcase of Preposition
   form
      constraints
         self.f.orth <-- "holding"
   meaning
      constraints
         self.m.name <-- "holding1"
