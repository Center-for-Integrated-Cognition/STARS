sp {task-utils*anystate*propose*generalize-task-goal
   (state <s> ^generalize-task-goal <gto>)
   (<gto> ^task-goal <goal>
          ^reference-task-op <ref-task>   
          ^reference-tcn-proc <ref-proc>
         -^general-goal-rep)
-->
   (<s> ^operator <o> + =)
   (<o> ^name generalize-task-goal
        ^destination <gto>
        ^task-goal <goal>
        ^reference-task-op <ref-task>   
        ^reference-tcn-proc <ref-proc>)
}
