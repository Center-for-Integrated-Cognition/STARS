sp {top-state*perception-monitor*predicate-monitor*elaborate*default*predicates
   (state <s> ^superstate nil
              ^perception-monitor.predicate-monitor <mon>)
-->
    (<mon> ^predicate-info <in-info>)
    (<in-info> ^predicate-handle on1)

    (<mon> ^predicate-info <right-of-info>)
    (<right-of-info> ^predicate-handle below1)
	
	(<mon> ^predicate-info <n-info>)
    (<n-info> ^predicate-handle near1)

    (<mon> ^predicate-info <d-info>)
    (<d-info> ^predicate-handle diagonal-with1)

	(<mon> ^predicate-info <li-info>)
    (<li-info> ^predicate-handle linear)

}
