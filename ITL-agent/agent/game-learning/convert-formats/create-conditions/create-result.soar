sp {create-condition*apply*create-result
   (state <s> ^name create-condition
   		  	 -^condition.name number
			  ^operator <o>)
   (<o> ^name create-result
   		^condition <c>)
-->
   (<c> ^result <r>)
   (<r> ^set <rs>)
}

