sp {top-state*elaborate*problem-space
   (state <s> ^superstate nil)
-->
   (<s> ^problem-space <ps>)
}

sp {top-state*elaborate*problem-space*attend-to-perception
   (state <s> ^superstate nil
              ^problem-space <ps>
             -^task-stack.top <> none)
-->
   (<ps> ^attend-to-perception yes)
}
