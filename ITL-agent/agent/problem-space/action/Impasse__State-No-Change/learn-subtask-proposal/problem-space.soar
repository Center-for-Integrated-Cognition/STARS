sp {learn-subtask-proposal*elaborate*default-problem-space*no
   (state <s> ^name learn-subtask-proposal)
-->
   (<s> ^default-problem-space no)

   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^execution-type simulate
         ^world.copy-type shallow)
}

sp {learn-subtask-proposal*elaborate*problem-space*propose-subtasks
   (state <s> ^name learn-subtask-proposal
              ^subtask-operator <sub>
              ^problem-space <ps>)
-->
   (<ps> ^propose-subtasks true)
}
