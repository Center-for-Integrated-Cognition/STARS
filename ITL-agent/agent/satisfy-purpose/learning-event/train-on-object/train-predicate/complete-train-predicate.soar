sp {train-predicate*propose*complete-train-predicate
   (state <s> ^name train-predicate)
-->
   (<s> ^operator <o> + <) 
   (<o> ^name complete-train-predicate)
}

sp {train-predicate*apply*complete-train-predicate
   (state <s> ^name train-predicate
              ^operator.name complete-train-predicate
              ^predicate <pred>
              ^superstate <ss>)
-->
   (<ss> ^trained <pred>)
}