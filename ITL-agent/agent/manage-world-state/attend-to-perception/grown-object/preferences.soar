sp {attend-to-perception*grown-object*prefer*intersect*over*overlap
   (state <s> ^name evaluate-grown-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name intersect-command)
   (<o2> ^name overlap-command)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {attend-to-perception*grown-object*prefer*overlap*intersect*over*change-world-state
   (state <s> ^name evaluate-grown-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << overlap-command intersect-command >>)
   (<o2> ^name change-world-state)
-->
   (<s> ^operator <o1> > <o2>)
}

