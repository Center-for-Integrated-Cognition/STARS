//  Concepts
schema Name
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- name1

schema Game
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- game1

schema Puzzle
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- puzzle1
