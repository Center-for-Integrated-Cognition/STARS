//  Concepts
construction NAME
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "name"
   meaning: Name

construction GAME
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "game"
   meaning: Game

construction PUZZLE
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "puzzle"
   meaning: Puzzle
