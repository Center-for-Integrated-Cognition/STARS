# Retrieve the parent word
sp {lookup-label*propose*smem-retrieve*word
   (state <s> ^name lookup-label
              ^sense.word <word>)
-->
   (<s> ^retrieve-lti <word>)
}

# Retrieve the property
sp {lookup-label*propose*smem-retrieve*property
   (state <s> ^name lookup-label
              ^sense.property <prop>)
-->
   (<s> ^retrieve-lti <prop>)
}
