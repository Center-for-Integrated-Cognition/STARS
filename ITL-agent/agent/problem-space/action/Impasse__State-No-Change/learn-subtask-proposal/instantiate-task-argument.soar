
sp {learn-subtask-proposal*elaborate*instantiate-task-argument*conditions
   (state <s> ^name learn-subtask-proposal
              ^subtask-tcn.conditions <conds>
              ^task-operator <task-op>
              ^procedural-id <proc-id>
              ^world <w>)
-->
   (<s> ^instantiate-task-argument <ins-arg>)
   (<ins-arg> ^arg-type conditions
              ^general-arg-rep <conds>)

   (<s> ^reference-tcn-proc <proc-id>
        ^reference-task-op <task-op>)
}

sp {learn-subtask-proposal*elaborate*predicate-test*conditions
   (state <s> ^name learn-subtask-proposal
              ^instantiate-task-argument <ins-arg>)
   (<ins-arg> ^arg-type conditions
              ^instantiation <i>)
-->
   (<s> ^predicate-test <i>)
   (<i> ^subtask-conditions true)
}
