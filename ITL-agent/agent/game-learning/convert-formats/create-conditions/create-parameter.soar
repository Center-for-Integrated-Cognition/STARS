sp {create-condition*apply*create-parameter
   (state <s> ^name create-condition
   		  	  ^parameter <p>
			  ^operator <o>)
   (<o> ^name create-parameter
   		^condition <c>)
-->
   (<c> ^parameter <p>)
}
sp {create-condition*apply*create-parameter*none
   (state <s> ^name create-condition
   		  	  -^parameter
			  ^operator <o>)
   (<o> ^name create-parameter
   		^condition <c>)
-->
   (<c> ^parameter <p>)
}